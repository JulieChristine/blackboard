{{#power-calendar
  selected=selected
  onSelect=(action (mut selected) value="moment") as |calendar|}}
  {{calendar.nav}}
  {{calendar.days}}

  {{log calendar.selected._i}}
{{/power-calendar}}

<ul>
  {{#each model as |event|}}
    {{event-tile event=event}}
  {{/each}}
  
  {{new-event saveEvent="saveEvent"}}
</ul>
